<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="teamProject.food114.mapper.BoardMapper">
	<!-- 공지사항 목록 -->
	<select id="selectBoardList" parameterType="hashmap" resultType="teamProject.food114.model.Board">
		SELECT *
		FROM FOOD114_BOARD
		WHERE CODE = 1
		
	</select>

	<!-- 게시글 개수  -->
	<select id="selectBoardListCnt" parameterType="hashmap" resultType="int">
		SELECT *
		FROM FOOD114_BOARD
		WHERE CODE = 1
		ORDER BY BOARDNO
		LIMIT 1 OFFSET 10
	</select>
	
	
	
	

</mapper>